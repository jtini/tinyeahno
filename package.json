{
  "name": "Tinyeahno",
  "description": "Personal website of Jeremy Tinianow",
  "version": "1.0.0",
  "author": "Jeremy Tinianow <jeremy@tinyeahno.com>",
  "dependencies": {
    "classnames": "^2.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.3",
    "gatsby": "^2.0.0",
    "gatsby-image": "^2.0.22",
    "gatsby-link": "^2.0.7",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.11",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-sitemap": "^2.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-custom-blocks": "^2.0.2",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.10",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "jest": {
    "transform": {
      ".(js|jsx)": "babel-jest",
      ".scss": "jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "jest",
    "test-watch": "jest --watch",
    "local": "gatsby develop",
    "deploy": "rm -rf .cache/* && gatsby build && gh-pages -d public"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "gatsby-plugin-flow": "^1.0.2",
    "gatsby-plugin-sass": "^2.0.5",
    "gh-pages": "^1.2.0",
    "jest": "^22.4.2",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.11.1",
    "react-test-renderer": "15.6.1"
  }
}
